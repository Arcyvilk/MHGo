{
  "name": "mhgo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "server/api.ts",
  "scripts": {
    "dev:client": "env vite --config vite.config.frontend.ts --port 3091",
    "dev:server": "env ENV=dev PORT=3092 nodemon --exec npx tsx server/api.ts",
    "dev": "concurrently 'yarn dev:client' 'yarn dev:server'",
    "build:client": "env REACT_APP_ENV=prod tsc && vite --config vite.config.frontend.ts build",
    "build:server": "tsc & tsc-alias",
    "build": "concurrently 'yarn build:client' 'yarn build:server'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.14.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "leaflet": "^1.9.4",
    "mongodb": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-explode": "^2.0.5",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.0",
    "react-toastify": "^9.1.3",
    "tsc-alias": "^1.8.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-mme": "^1.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "ts-node": "^10.9.2",
    "typescript": "*",
    "vite": "^5.0.8",
    "vite-plugin-node": "^3.1.0",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-mme"
    ]
  }
}
