{
  "name": "mhgo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "server/index.ts",
  "scripts": {
    "dev:frontend": "vite --config vite.config.frontend.ts --port 3091",
    "dev:backend": "vite --config vite.config.backend.ts server/index.ts --port 3092",
    "dev": "concurrently 'yarn dev:frontend' 'yarn dev:backend'",
    "build:frontend": "tsc && vite --config vite.config.frontend.ts build",
    "build:backend": "tsc && vite --config vite.config.backend.ts server/index.ts build",
    "build": "concurrently 'yarn build:frontend' 'yarn build:backend'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.14.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-mme": "^1.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "ts-node": "^10.9.2",
    "typescript": "*",
    "vite": "^5.0.8",
    "vite-plugin-node": "^3.1.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-mme"
    ]
  }
}
